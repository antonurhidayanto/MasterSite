{
  "name": "master-site-menu-remote",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "clean:all": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && rm -rf dist",
    "updateTypes": "npm uninstall mf-simdev-types && npm install mf-simdev-types",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "exposed": "vite build && vite preview --host"
  },
  "dependencies": {
    "@originjs/vite-plugin-federation": "^1.3.9",
    "@vitejs/plugin-react": "^4.3.4",
    "js-cookie": "^3.0.5",
    "mf-simdev-types": "^0.1.53",
    "motion": "^11.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "vite": "^6.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2"
